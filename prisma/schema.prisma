datasource db {
  provider = "sqlite"
  url      = "file:./prod.db"
}

generator client {
  provider = "prisma-client-js"
}

model Video {
  id           String   @id @default(cuid())
  title        String
  prompt       String
  youtubeUrl   String
  thumbnailUrl String
  createdAt    DateTime @default(now())
}